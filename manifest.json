{
  "manifest_version": 3,
  "name": "Storage Cleaner",
  "version": "1.0",
  "permissions": ["scripting", "storage", "tabs", "contextMenus", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "images/favicon16.png",
    "32": "images/favicon32.png",
    "96": "images/favicon96.png"
  }
}
